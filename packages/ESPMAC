         SR    R2,R15                                                   01220000
         LR    R3,R15                                                   01230000
         BCTR  R3,0                                                     01240000
&INMOVE  SETB  1                                                GP09347 01250000
         EX    R3,DCONMOVE                                              01260000
         LM    R14,R3,12(R13)                                           01270000
         BR    R14                                                      01280000
         SPACE                                                          01290000
.OK6Z    AIF   ('&DB' NE '').OKWK                                88025  01300000
DCONDB   DC    D'0'          LOCAL WORK AREA                     88025  01310000
.OKWK    ANOP  ,                                                 88025  01320000
