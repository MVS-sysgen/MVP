.PROC    AIF   ('&PRINT' EQ 'NOGEN' OR '&PRINT' EQ 'NODATA').MEND       09340000
.*                                                                      09360000
.*                                                                      09380000
         AIF   (K'&TITLE LE 0).NTITL                                    09400000
.*                                                                      09420000
         AIF   ('&TITLE' NE 'EJECT').NEJEC                              09440000
         EJECT                                                          09460000
         AGO   .NTITL                                                   09480000
.*                                                                      09500000
.NEJEC   ANOP                                                           09520000
&TITLE(1) TITLE &TITLE(2)                                               09540000
.*                                                                      09560000
.*                                                                      09580000
.NTITL   ANOP                                                           09600000
.*                                                                      09620000
.*                                                                      09640000
&K       SETA  K'&STRING                                                09660000
         AIF   ((&K EQ 0) OR (&K GT 64)).DONE                           09680000
.*                                                                      09700000
.*                                                                      09720000
         AIF   ('&BLOCK' EQ 'NO').NBLK1                                 09740000
 MNOTE   '**************************************************************09760000
               **********'                                              09780000
&B       SETC  '*'                                                      09800000
.NBLK1   ANOP  ,                                                        09820000
.*                                                                      09840000
.*                                                                      09860000
.*                                                                      09880000
&M       SETA  1                                                        09900000
.COPY2   ANOP  ,                                                        09920000
&STR(&M) SETC  '&STRING'(&M,1)                                          09940000
&M       SETA  &M+1                                                     09960000
         AIF   (&M LE &K).COPY2                                         09980000
.*                                                                      10000000
&J       SETA  0                                                        10020000
&M       SETA  0                                                        10040000
.*                                                                      10060000
.*                                                                      10080000
.EDIT    AIF   (&M EQ &K).AGIN                                          10100000
&M       SETA  &M+1                                                     10120000
&I       SETA  &M                                                       10140000
         AIF   ('&STR(&M)' NE '''').EDIT                                10160000
.*                                                                      10180000
         AIF   (&J GT 0).QTCK                                           10200000
.*                                                                      10220000
&J       SETA  1                                                        10240000
         AGO   .COMP                                                    10260000
.*                                                                      10280000
.*                                                                      10300000
.QTCK    AIF   ('&STR(&M+1)' NE '''').COMP                              10320000
.*                                                                      10340000
&STR(&M) SETC  '"'                                                      10360000
&J       SETA  0                                                        10380000
&M       SETA  &M+1                                                     10400000
&I       SETA  &M                                                       10420000
.*                                                                      10440000
.*                                                                      10460000
.COMP    ANOP                                                           10480000
&STR(&I) SETC  '&STR(&I+1)'                                             10500000
&I       SETA  &I+1                                                     10520000
         AIF   (&I LT &K).COMP                                          10540000
.*                                                                      10560000
&STR(&K) SETC  ' '                                                      10580000
&K       SETA  &K-1                                                     10600000
&M       SETA  &M-1                                                     10620000
         AGO   .EDIT                                                    10640000
.*                                                                      10660000
.*                                                                      10680000
.AGIN    ANOP                                                           10700000
&I       SETA  1                                                        10720000
&L       SETA  &K-&N                                                    10740000
         AIF   (&L LE 0).ENDOFF                                         10760000
.*                                                                      10780000
         AIF   (&L LE 8).CK4                                            10800000
.*                                                                      10820000
&L       SETA  8                                                        10840000
.*                                                                      10860000
.*                                                                      10880000
.CK4     ANOP                                                           10900000
&J       SETA  (8-&L)/2*8                                               10920000
.*                                                                      10940000
.*                                                                      10960000
.FILT    AIF   (&I GT &J).SETL                                          10980000
.*                                                                      11000000
&F(&I)   SETC  '        '                                               11020000
&F(&I+1) SETC  '        '                                               11040000
&F(&I+2) SETC  '        '                                               11060000
&F(&I+3) SETC  '        '                                               11080000
&F(&I+4) SETC  '        '                                               11100000
&F(&I+5) SETC  '        '                                               11120000
&F(&I+6) SETC  '        '                                               11140000
&F(&I+7) SETC  '        '                                               11160000
&I       SETA  &I+8                                                     11180000
         AGO   .FILT                                                    11200000
.*                                                                      11220000
.*                                                                      11240000
.SETL    ANOP                                                           11260000
&L       SETA  &L+&N                                                    11280000
.*                                                                      11300000
.*                                                                      11320000
.LOOP    AIF   (&N EQ &L).FINI                                          11340000
.*                                                                      11360000
&N       SETA  &N+1                                                     11380000
&J       SETA  1                                                        11400000
&W       SETC  '&STR(&N)'                                               11420000
.*                                                                      11440000
.*                                                                      11460000
.MTCH    AIF   ('&W' EQ '&HEADC(&J)').GO                                11480000
.*                                                                      11500000
&J       SETA  &J+9                                                     11520000
         AIF   ('&HEADC(&J)' NE 'END').MTCH                             11540000
&J       SETA  &J-9                                                     11560000
.*                                                                      11580000
.*                                                                      11600000
.GO      ANOP                                                           11620000
.*                                                                      11640000
&F(&I)   SETC  '&HEADC(&J+1)'                                           11660000
&F(&I+1) SETC  '&HEADC(&J+2)'                                           11680000
&F(&I+2) SETC  '&HEADC(&J+3)'                                           11700000
&F(&I+3) SETC  '&HEADC(&J+4)'                                           11720000
&F(&I+4) SETC  '&HEADC(&J+5)'                                           11740000
&F(&I+5) SETC  '&HEADC(&J+6)'                                           11760000
&F(&I+6) SETC  '&HEADC(&J+7)'                                           11780000
&F(&I+7) SETC  '&HEADC(&J+8)'                                           11800000
&I       SETA  &I+8                                                     11820000
         AGO   .LOOP                                                    11840000
.*                                                                      11860000
.*                                                                      11880000
.FINI    AIF   (&I GT 64).DOIT                                          11900000
.*                                                                      11920000
&F(&I)   SETC  '        '                                               11940000
&F(&I+1) SETC  '        '                                               11960000
&F(&I+2) SETC  '        '                                               11980000
&F(&I+3) SETC  '        '                                               12000000
&F(&I+4) SETC  '        '                                               12020000
&F(&I+5) SETC  '        '                                               12040000
&F(&I+6) SETC  '        '                                               12060000
&F(&I+7) SETC  '        '                                               12080000
&I       SETA  &I+8                                                     12100000
         AGO   .FINI                                                    12120000
.*                                                                      12140000
.*                                                                      12160000
.DOIT    AIF   ('&BLOCK' EQ 'NO').SPACE                                 12180000
 MNOTE   '*                                                            *12200000
                        *'                                              12220000
 MNOTE   '*                                                            *12240000
                        *'                                              12260000
         AGO   .DOIT2                                                   12280000
.SPACE   ANOP   ,                                                       12300000
         SPACE 2                                                        12320000
.DOIT2   ANOP  ,                                                        12340000
 MNOTE   '&B  &F(1)&F(9)&F(17)&F(25)&F(33)&F(41)&F(49)&F(57)   &B'      12360000
 MNOTE   '&B  &F(2)&F(10)&F(18)&F(26)&F(34)&F(42)&F(50)&F(58)   &B'     12380000
 MNOTE   '&B  &F(3)&F(11)&F(19)&F(27)&F(35)&F(43)&F(51)&F(59)   &B'     12400000
 MNOTE   '&B  &F(4)&F(12)&F(20)&F(28)&F(36)&F(44)&F(52)&F(60)   &B'     12420000
 MNOTE   '&B  &F(5)&F(13)&F(21)&F(29)&F(37)&F(45)&F(53)&F(61)   &B'     12440000
 MNOTE   '&B  &F(6)&F(14)&F(22)&F(30)&F(38)&F(46)&F(54)&F(62)   &B'     12460000
 MNOTE   '&B  &F(7)&F(15)&F(23)&F(31)&F(39)&F(47)&F(55)&F(63)   &B'     12480000
