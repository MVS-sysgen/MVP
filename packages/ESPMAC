&NM      $TCKCONV STCKVAL=0,CONVVAL=DB3,TIMETYP=BIN,DATETYP=YYYYMMDD    00030000
 PRTDATA ' At the tone the time will be',(DB3,TIME,PAD),'on',          *00040000
