         AIF   ('&TOKEN   '(1,3) EQ 'J2/').SETJES2               85076  00910000
         AIF   (K'&TOKEN NE 6 AND K'&TOKEN NE 8).NOTPROP         82099  00920000
         AIF   ('&TOKEN'(1,1) NE 'P').NOTPROP                    82099  00930000
         AIF   ('&TOKEN'(3,1) NE '/').NOTPROP                    82099  00940000
