./ ADD NAME=WTPVT
         MACRO                                                          00001000
&L       WTPVT &TYPE,&PFX=,&LPFX=0,&SFX=,&LSFX=0                        00002000
         AIF   ('&TYPE' EQ 'CODE').CODE                                 00003000
         AIF   ('&TYPE' EQ 'DSECT').DSECT                               00004000
