//BSPFCOOK JOB (JOB),
//             'INSTALL BRODSCAN',
//             CLASS=A,
//             MSGCLASS=A,
//             MSGLEVEL=(1,1),
//             USER=IBMUSER,
//             PASSWORD=SYS1
//SOURCE EXEC PGM=PDSLOAD
//STEPLIB  DD  DSN=SYSC.LINKLIB,DISP=SHR
//SYSPRINT DD  SYSOUT=*
//SYSUT2   DD  DISP=(,PASS),UNIT=VIO,
//             SPACE=(CYL,(2,1,2)),
//             DCB=(LRECL=80,RECFM=FB,BLKSIZE=3120,DSORG=PO)
//SYSUT1   DD DATA,DLM=@@
./ ADD NAME=BSPFCOO1
         TITLE 'Table of message pointers and texts'
*********************************************************************
*        Printing has been turned off
*********************************************************************
         PRINT OFF
         DS    0F
MLTBL    DC    A(ML00000)
         DC    A(ML00001)
         DC    A(ML00002)
         DC    A(ML00003)
